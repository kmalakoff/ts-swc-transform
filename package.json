{
  "name": "ts-swc-transform",
  "version": "2.6.25",
  "description": "Typescript transformers for swc. Supports Node >= 0.8",
  "keywords": [
    "matcher",
    "swc",
    "typescript",
    "transform",
    "transformSync",
    "transformFile",
    "transformDirectory",
    "directory",
    "file"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-transform",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-swc-transform.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe",
    "prepublishOnly": "tsds validate",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines tsds test:node --no-timeouts",
    "version": "tsds version"
  },
  "dependencies": {
    "@swc/core": "*",
    "core-js-pure": "*",
    "exit": "*",
    "fs-iterator": "*",
    "install-optional": "*",
    "is-absolute": "*",
    "lodash.debounce": "*",
    "lodash.find": "*",
    "mkdirp-classic": "*",
    "node-version-call": "*",
    "queue-cb": "*",
    "read-tsconfig-sync": "*",
    "resolve": "*",
    "rimraf2": "*",
    "starts-with": "^1.0.2",
    "test-match": "*",
    "ts-node": "*",
    "typescript": "*"
  },
  "devDependencies": {
    "@biomejs/biome": "*",
    "@types/mocha": "*",
    "@types/node": "*",
    "core-js": "*",
    "cr": "*",
    "cross-spawn-cb": "*",
    "node-version-use": "*",
    "pinkie-promise": "*",
    "react": "*",
    "ts-dev-stack": "*"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
