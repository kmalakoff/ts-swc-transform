    "version": "tsds version"
  },
  "dependencies": {
    "@swc/core": "^1.15.3",
    "core-js-pure": "^3.47.0",
    "exit-compat": "^1.0.0",
    "fs-iterator": "^6.1.10",
    "install-optional": "^1.0.16",
    "is-absolute": "^1.0.0",
    "lodash.debounce": "^4.0.8",
    "lodash.find": "^4.6.0",
    "mkdirp-classic": "^0.5.3",
    "node-version-call": "^1.9.19",
    "queue-cb": "^1.6.1",
    "read-tsconfig-sync": "^1.1.6",
    "resolve": "^1.22.11",
    "test-match": "^1.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.0.0",
    "core-js": "^3.47.0",
