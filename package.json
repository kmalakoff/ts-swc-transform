{
  "name": "ts-swc-transform",
  "version": "1.9.3",
  "description": "Typescript transformers for swc. Supports Node >= 0.8",
  "keywords": [
    "matcher",
    "swc",
    "typescript",
    "transform",
    "transformSync",
    "transformDirectory",
    "directory",
    "transformFile",
    "file"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-transform",
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/ts-swc-transform.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./*": "./*"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "bin": {
    "tst": "bin/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsds build",
    "depcheck": "depcheck",
    "deploy": "tsds deploy",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "",
    "version2": "tsds version"
  },
  "dependencies": {
    "@swc/core": "*",
    "call-once-fn": "^1.0.1",
    "cross-spawn-cb": "^1.5.7",
    "exit": "^0.1.2",
    "fs-iterator": "^5.2.1",
    "get-tsconfig-compat": "^2.0.2",
    "install-module-linked": "^0.2.0",
    "is-absolute": "^1.0.0",
    "lazy-cache": "^2.0.2",
    "minimatch": "^3.1.2",
    "mkdirp-classic": "^0.5.3",
    "node-version-call": "^1.3.5",
    "path-posix": "^1.0.0",
    "queue-cb": "^1.4.4",
    "resolve": "^1.22.10",
    "resolve-once-cb": "^0.1.2",
    "rimraf2": "^2.8.2",
    "ts-node": "*",
    "typescript": "*",
    "unixify": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/core": "^1.9.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "cr": "^0.1.0",
    "es6-map": "^0.1.5",
    "es6-symbol": "^3.1.4",
    "lodash.difference": "^4.5.0",
    "object-assign": "^4.1.1",
    "pinkie-promise": "^2.0.1",
    "react": "^19.0.0",
    "ts-node": "^10.8.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
