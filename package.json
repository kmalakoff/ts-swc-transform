{
  "name": "ts-swc-transform",
  "version": "1.15.0",
  "description": "Typescript transformers for swc. Supports Node >= 0.8",
  "keywords": [
    "matcher",
    "swc",
    "typescript",
    "transform",
    "transformSync",
    "transformFile",
    "transformDirectory",
    "directory",
    "file"
  ],
  "homepage": "https://github.com/kmalakoff/ts-swc-transform",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/kmalakoff/ts-swc-transform.git"
  },
  "license": "MIT",
  "author": "Kevin Malakoff <kmalakoff@gmail.com> (https://github.com/kmalakoff)",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.mjs",
    "require": "./dist/cjs/index.cjs",
    "types": "./dist/types/index.d.ts"
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "scripts",
    "assets"
  ],
  "scripts": {
    "build": "tsds build",
    "format": "biome check --write --unsafe src/ test/",
    "test": "tsds test:node --no-timeouts",
    "test:engines": "nvu engines npm test",
    "version": "tsds version"
  },
  "dependencies": {
    "@swc/core": "*",
    "core-js-pure": "^3.40.0",
    "exit": "^0.1.2",
    "fs-iterator": "^5.2.10",
    "get-tsconfig-compat": "^2.0.3",
    "install-optional": "^0.1.0",
    "is-absolute": "^1.0.0",
    "minimatch": "^3.1.2",
    "mkdirp-classic": "^0.5.3",
    "path-posix": "^1.0.0",
    "queue-cb": "^1.4.10",
    "resolve": "^1.22.10",
    "rimraf2": "^2.8.2",
    "ts-node": "*",
    "typescript": "*",
    "unixify": "^1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@swc/core": "^1.9.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.5",
    "cr": "^0.1.0",
    "cross-spawn-cb": "^2.1.2",
    "lodash.difference": "^4.5.0",
    "pinkie-promise": "^2.0.1",
    "react": "^19.0.0",
    "ts-node": "^10.8.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=0.8"
  },
  "tsds": {
    "source": "src/index.ts"
  }
}
